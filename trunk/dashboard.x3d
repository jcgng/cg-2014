        <ROUTE fromField='triggerTime' fromNode='TRIGGER_ShowBillboard3' toField='set_startTime' toNode='CLOCK_InterpolatorShow3'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_ShowBillboard3' toField='set_enabled' toNode='CLOCK_InterpolatorShow3'/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_ShowBillboard3' toField='set_startTime' toNode='CLOCK_InterpolatorFade3'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_ShowBillboard3' toField='set_enabled' toNode='CLOCK_InterpolatorFade3'/>
        <ROUTE fromField='fraction_changed' fromNode='CLOCK_InterpolatorShow3' toField='set_fraction' toNode='ScalarInterpolatorShow3'/>
        <ROUTE fromField='value_changed' fromNode='ScalarInterpolatorShow3' toField='set_transparency' toNode='BillboardTextMaterial3'/>
        <ROUTE fromField='fraction_changed' fromNode='CLOCK_InterpolatorFade3' toField='set_fraction' toNode='ScalarInterpolatorFade3'/>
