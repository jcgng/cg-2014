    <Scene>        
        <!--- ###### MAIN DASHBOARD ####### -->
        <!--    
        <Billboard DEF="dashboard" axisOfRotation="0 0 0">
        -->
        <Transform rotation='1 0 0 1.57'>
        <Transform rotation='0 1 0 3.14'> 
        <Transform scale="0.5 0.5 0.5"> 
            <!-- BACKPLANE -->
            <Shape>
                <Box size="4,2,0.1"/>
                <Appearance>
                    <!--<Material DEF="BackPlaneMaterial" ambientIntensity='0.022727' diffuseColor='0 0 0' shininess='1' specularColor='0 0 0' transparency='0'/>-->
                    <ImageTexture DEF="BackPlaneMaterial" url=' "images/background_panel.png" '/>
                </Appearance>
            </Shape>
            <!-- CYLINDRICAL BORDERS --> 
            <Transform translation='-2.0 0 0'> 
                <Shape DEF="verticalBorder">
                    <Cylinder radius="0.1" height="2"/>
                    <Appearance>
                        <Material DEF="CylindricalBordersMaterial" ambientIntensity='0.022727' diffuseColor='0.314286 0.074365 0.2' shininess='0.04' specularColor='0.5 0.5 0.5' transparency='0'/>       
                    </Appearance>
                </Shape>
            </Transform>
            <Transform translation='2.0 0 0'> 
                <Shape USE="verticalBorder" />
            </Transform>
            <Transform rotation='0 0 1 -1.57' translation='0 1.0 0'> 
                <Shape DEF="horizontalBorder">
                    <Cylinder radius="0.1" height="4"/>
                    <Appearance>
                        <Material USE="CylindricalBordersMaterial"/>
                    </Appearance>
                </Shape>
            </Transform> 
            <Transform rotation='0 0 1 -1.57' translation='0 -1.0 0'> 
                <Shape USE="horizontalBorder"/>
            </Transform>
            <!-- SPHERIC CORNERS --> 
            <Transform translation='-2.0 -1.0 0'>
                <Shape DEF="corner">
                    <Sphere radius="0.1"/>
                    <Appearance>
                        <Material USE="CylindricalBordersMaterial"/>
                    </Appearance>
                </Shape>
            </Transform>

            <Transform translation='2.0 -1.0 0'>
                <Shape USE="corner"/>
            </Transform>

            <Transform translation='-2.0 1.0 0'>
                <Shape USE="corner"/>
            </Transform>

            <Transform translation='2.0 1.0 0'>
                <Shape USE="corner"/>
            </Transform>    
            
            <!--- ###### DISPLAYS ##### -->
            <Transform translation='-1.2 0 0.1'>
                <Shape>
                   <Box size="0.8,0.8,0"/>
                   <Appearance>
                        <Material diffuseColor='1 1 1'/>
                        <ImageTexture DEF='patientPic'/>
                    </Appearance>
                </Shape>
                <Transform rotation='0 0 0 0' translation='0 -0.6 0'>
                    <Shape>
                        <Text DEF="patientName1" string="">
                            <FontStyle DEF='CenteredFontStyle' justify='"MIDDLE" "MIDDLE"' size="0.3"/>
                        </Text> 
                        <Appearance>
                            <Material DEF='BillboardTextMaterial1' emissiveColor='1 .8 .5' transparency="1"/>
                        </Appearance>
                    </Shape>
                     <Shape>
                        <Text DEF="patientName2" string="">
                            <FontStyle USE='CenteredFontStyle'/>
                        </Text> 
                        <Appearance>
                            <Material DEF='BillboardTextMaterial2' emissiveColor='1 .8 .5' transparency="1"/>
                        </Appearance>
                    </Shape>
                     <Shape>
                        <Text DEF="patientName3" string="">
                            <FontStyle USE='CenteredFontStyle'/>
                        </Text> 
                        <Appearance>
                            <Material DEF='BillboardTextMaterial3' emissiveColor='1 .8 .5' transparency="1"/>
                        </Appearance>
                    </Shape>
                    <Transform rotation='0 0 0 0' translation='0 1.3 0'>
                        <Shape>
                            <Text DEF="patientAge1" string="">
                                 <FontStyle USE='CenteredFontStyle'/>
                            </Text> 
                            <Appearance>
                                <Material USE='BillboardTextMaterial1'/>
                            </Appearance>
                        </Shape>
                        <Shape>
                            <Text DEF="patientAge2" string="">
                                 <FontStyle USE='CenteredFontStyle'/>
                            </Text> 
