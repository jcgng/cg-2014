        </Transform>    
        </Transform>
        <!--
        </Billboard>
        -->
        <BooleanFilter DEF='BOOL_ShowBillboard1'/>
        <TimeTrigger DEF='TRIGGER_ShowBillboard1'/>
        <BooleanFilter DEF='BOOL_ShowBillboard2'/>
        <TimeTrigger DEF='TRIGGER_ShowBillboard2'/>
        <BooleanFilter DEF='BOOL_ShowBillboard3'/>
        <TimeTrigger DEF='TRIGGER_ShowBillboard3'/>
        
        <BooleanFilter DEF='BOOL_FadeBillboard1'/>
        <TimeTrigger DEF='TRIGGER_FadeBillboard1'/>
        <BooleanFilter DEF='BOOL_FadeBillboard2'/>
        <TimeTrigger DEF='TRIGGER_FadeBillboard2'/>
        <BooleanFilter DEF='BOOL_FadeBillboard3'/>
        <TimeTrigger DEF='TRIGGER_FadeBillboard3'/>
        
        <TimeSensor DEF='CLOCK_InterpolatorFade1' cycleInterval='1' enabled='false' loop='false'/>
        <TimeSensor DEF='CLOCK_InterpolatorShow1' cycleInterval='1' enabled='false' loop='false'/>
        <ScalarInterpolator DEF='ScalarInterpolatorShow1' key='0.0 0.5 1.0' keyValue='1 0.5 0'/>
        <ScalarInterpolator DEF='ScalarInterpolatorFade1' key='1.0' keyValue='1'/>
        <TimeSensor DEF='CLOCK_InterpolatorFade2' cycleInterval='1' enabled='false' loop='false'/>
        <TimeSensor DEF='CLOCK_InterpolatorShow2' cycleInterval='1' enabled='false' loop='false'/>
        <ScalarInterpolator DEF='ScalarInterpolatorShow2' key='0.0 0.5 1.0' keyValue='1 0.5 0'/>
        <ScalarInterpolator DEF='ScalarInterpolatorFade2' key='1.0' keyValue='1'/>
        <TimeSensor DEF='CLOCK_InterpolatorFade3' cycleInterval='1' enabled='false' loop='false'/>
        <TimeSensor DEF='CLOCK_InterpolatorShow3' cycleInterval='1' enabled='false' loop='false'/>
        <ScalarInterpolator DEF='ScalarInterpolatorShow3' key='0.0 0.5 1.0' keyValue='1 0.5 0'/>
        <ScalarInterpolator DEF='ScalarInterpolatorFade3' key='1.0' keyValue='1'/>
       
        <!-- Routes -->
        <ROUTE fromField="inputTrue" fromNode="BOOL_FadeBillboard1" toField="set_boolean" toNode="TRIGGER_FadeBillboard1"/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_FadeBillboard1' toField='set_startTime' toNode='CLOCK_InterpolatorFade1'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_FadeBillboard1' toField='set_enabled' toNode='CLOCK_InterpolatorFade1'/>
        <ROUTE fromField="inputTrue" fromNode="BOOL_FadeBillboard2" toField="set_boolean" toNode="TRIGGER_FadeBillboard2"/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_FadeBillboard2' toField='set_startTime' toNode='CLOCK_InterpolatorFade2'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_FadeBillboard2' toField='set_enabled' toNode='CLOCK_InterpolatorFade2'/>
        <ROUTE fromField="inputTrue" fromNode="BOOL_FadeBillboard3" toField="set_boolean" toNode="TRIGGER_FadeBillboard3"/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_FadeBillboard3' toField='set_startTime' toNode='CLOCK_InterpolatorFade3'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_FadeBillboard3' toField='set_enabled' toNode='CLOCK_InterpolatorFade3'/>
        
        <ROUTE fromField="inputTrue" fromNode="BOOL_ShowBillboard1" toField="set_boolean" toNode="TRIGGER_ShowBillboard1"/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_ShowBillboard1' toField='set_startTime' toNode='CLOCK_InterpolatorShow1'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_ShowBillboard1' toField='set_enabled' toNode='CLOCK_InterpolatorShow1'/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_ShowBillboard1' toField='set_startTime' toNode='CLOCK_InterpolatorFade1'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_ShowBillboard1' toField='set_enabled' toNode='CLOCK_InterpolatorFade1'/>
        <ROUTE fromField='fraction_changed' fromNode='CLOCK_InterpolatorShow1' toField='set_fraction' toNode='ScalarInterpolatorShow1'/>
        <ROUTE fromField='value_changed' fromNode='ScalarInterpolatorShow1' toField='set_transparency' toNode='BillboardTextMaterial1'/>
        <ROUTE fromField='fraction_changed' fromNode='CLOCK_InterpolatorFade1' toField='set_fraction' toNode='ScalarInterpolatorFade1'/>
        <ROUTE fromField='value_changed' fromNode='ScalarInterpolatorFade1' toField='set_transparency' toNode='BillboardTextMaterial2'/>
        <ROUTE fromField='value_changed' fromNode='ScalarInterpolatorFade1' toField='set_transparency' toNode='BillboardTextMaterial3'/>
        
        <ROUTE fromField="inputTrue" fromNode="BOOL_ShowBillboard2" toField="set_boolean" toNode="TRIGGER_ShowBillboard2"/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_ShowBillboard2' toField='set_startTime' toNode='CLOCK_InterpolatorShow2'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_ShowBillboard2' toField='set_enabled' toNode='CLOCK_InterpolatorShow2'/>
        <ROUTE fromField='triggerTime' fromNode='TRIGGER_ShowBillboard2' toField='set_startTime' toNode='CLOCK_InterpolatorFade2'/>
        <ROUTE fromField='inputTrue' fromNode='BOOL_ShowBillboard2' toField='set_enabled' toNode='CLOCK_InterpolatorFade2'/>
        <ROUTE fromField='fraction_changed' fromNode='CLOCK_InterpolatorShow2' toField='set_fraction' toNode='ScalarInterpolatorShow2'/>
        <ROUTE fromField='value_changed' fromNode='ScalarInterpolatorShow2' toField='set_transparency' toNode='BillboardTextMaterial2'/>
        <ROUTE fromField='fraction_changed' fromNode='CLOCK_InterpolatorFade2' toField='set_fraction' toNode='ScalarInterpolatorFade2'/>
