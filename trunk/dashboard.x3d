                            </Appearance>
                        </Shape>
                        <Shape>
                            <Text DEF="patientAge3" string="">
                                 <FontStyle USE='CenteredFontStyle'/>
                            </Text> 
                            <Appearance>
                                <Material USE='BillboardTextMaterial3'/>
                            </Appearance>
                        </Shape>
                    </Transform>
                </Transform>
            </Transform>
    
            <Transform translation='-0.2 0.6 0.1'>
                <Shape>
                    <Box size="0.5,0.5,0"/>
                    <Appearance>
                        <Material ambientIntensity='0.022727' diffuseColor='0 0 0' shininess='1' specularColor='1 1 1' transparency='0'/>
                        <ImageTexture DEF='heart' url=' "images/heart.png" '/>
                    </Appearance>
                </Shape>
            </Transform>
    
            <Transform translation='-0.2 -0.5 0.15'>
                <Shape>
                    <Cylinder radius="0.08" height="0.8"/>
                    <Appearance>
			<Material diffuseColor='1 1 1' transparency='0.5'/>
                    </Appearance>
                </Shape>
                <Transform rotation='0 0 0 0' translation='0 -0.1 0'>
                    <Shape>
                        <Cylinder DEF="termFill" radius="0.06" height="0.2"/>
                        <Appearance>
                            <Material DEF="termMaterial" ambientIntensity='0.022727' diffuseColor='1 0 0' shininess='1' specularColor='1 1 1' transparency='0'/>
                        </Appearance>
                    </Shape>
                    <Transform rotation='0 0 0 0' translation='0 -0.2 0'>
                        <Shape>             
                            <Sphere radius="0.15"/>
                            <Appearance>
                                <Material USE="termMaterial" />
                           </Appearance>
                        </Shape>
                    </Transform>   
                </Transform>
            </Transform>
    
   <!--<Transform rotation='0 0 0 0' translation='0 0 0'>        
    <Transform rotation='0 0 0 0' translation='-0.15 0 0'>
                    <Shape DEF="heart_ball">             
                        <Sphere radius="0.25"/>
                            <Appearance>
                                <Material ambientIntensity='0.022727' diffuseColor='1 0 0' shininess='1' specularColor='1 1 1' transparency='0'/>
                            </Appearance>
                    </Shape>
     </Transform>
            
     <Transform rotation='0 0 0 0' translation='0.15 0 -0'>
                    <Shape DEF="heart_ball2">             
                        <Sphere radius="0.25"/>
                            <Appearance>
                                <Material ambientIntensity='0.022727' diffuseColor='1 0 0' shininess='1' specularColor='1 1 1' transparency='0'/>
                            </Appearance>
                    </Shape>
     </Transform>
     
     <Transform rotation='0 0 0 0' translation='0 -0.14 0'>
                    <Shape DEF="heart_ball3">             
                        <Sphere radius="0.245"/>
                            <Appearance>
                                <Material ambientIntensity='0.022727' diffuseColor='1 0 0' shininess='1' specularColor='1 1 1' transparency='0'/>
                            </Appearance>
                    </Shape>
     </Transform>
     
     <Transform rotation='1 0 0 -3.14' translation='0 -0.45 0'>
                <Shape DEF="heart_cone">             
                    <Cone bottomRadius="0.25" height="0.45"/>
                        <Appearance>
			<Material ambientIntensity='0.022727' diffuseColor='1 0 0' shininess='1' specularColor='1 1 1' transparency='0'/>
                        </Appearance>
                </Shape>
      </Transform>     
    </Transform>  -->
    
            <Transform translation='1.5 0.6 0.1'>
                <Shape>
                    <Text DEF="BPM_label" string="BPM">
                         <FontStyle DEF='labelstyle' justify='"MIDDLE" "MIDDLE"' size="0.3"/>
                    </Text> 
                    <Appearance>
                        <Material emissiveColor='1 1 0'/>
                    </Appearance>
                </Shape>
               
                <Transform rotation='0 0 0 0' translation='-1.0 0 0.0'>
                    <Shape>
                        <Text DEF="BPM1_value" string="">
